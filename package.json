{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon --watch ./server server.js",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint src"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "nodemon": "1.x.x",
    "babel-eslint": "^3.1.9",
    "eslint-plugin-react": "^2.3.0"
  },
  "dependencies": {
    "babel": "5.x.x",
    "babel-core": "5.x.x",
    "babel-loader": "^5.1.2",
    "babel-runtime": "5.x.x",
    "body-parser": "1.x.x",
    "graphql": "0.4.x",
    "lokka": "1.x.x",
    "lokka-transport-http": "1.x.x",
    "react": "^0.13.0",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^1.3.0",
    "react-modal": "^3.5.1",
    "underscore": "1.8.x",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  }
}
